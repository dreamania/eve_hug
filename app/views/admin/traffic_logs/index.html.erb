<div class="row-fluid">
  <!-- block -->
  <div class="block">
    <div class="navbar navbar-inner block-header">
      <div class="muted pull-left">채널별 유입 목록</div>
    </div>
    <div class="row-fluid span11">
      <p class="total_user_header"><채널별 유입 현황></p>
      <table cellpadding="0" cellspacing="0" border="0" class="table table-striped table-bordered traffic-logs">
        <thead>
          <tr>
            <th>날짜</th>
            <th class="home">이브자리 홈</th>
            <th class="ts">감사메세지 홈</th>
            <th class="fb_share">공유하기 홈</th>
            <th class="ks">카카오스토리</th>
            <th>기타</th>
            <th>합계</th>
          </tr>
        </thead>
        <tbody>
           <% @traffic_stats.each do |t| %>
					 <tr>
						 <td>
							 <span><%=t.created_date%></span>  
						 </td> 
						 <td class="home">
							 <span><%=t.home_count%></span>  
						 </td>
						 <td class="ts">
							 <span><%=t.ts_count%></span>  
						 </td>
						 <td class="fb_share">
							 <span><%=t.fb_share_count%></span>  
						 </td>
						 <td class="ks">
							 <span><%=t.ks_count%></span>  
						 </td>
						 <td>
							 <span><%=t.etc_count%></span>  
						 </td>
						 <td>
							 <span><%=t.total_count%></span>  
						 </td>
					 </tr>
					 <% end %>
					 <% @traffic_stats_sum.each do |t| %>
					 <tr>
						 <td>
							 <span>총합계</span>  
						 </td> 
						 <td>
							 <span><%=t.home_count%></span>  
						 </td>
						 <td>
							 <span><%=t.ts_count%></span>  
						 </td>
						 <td>
							 <span><%=t.fb_share_count%></span>  
						 </td>
						 <td>
							 <span><%=t.ks_count%></span>  
						 </td>
						 <td>
							 <span><%=t.etc_count%></span>  
						 </td>
						 <td>
							 <span><%=t.total_count%></span>  
						 </td>
					 </tr>
          <% end %>
        </tbody>
      </table>
    </div>
     
    <div class="row-fluid block-content collapse in">

    </div>
  </div>
  <!-- /block -->
</div>
