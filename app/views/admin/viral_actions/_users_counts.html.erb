<p class="total_user_header"><참여자 현황></p>
<table cellpadding="0" cellspacing="0" border="0" class="table table-striped table-bordered">
  <thead>
    <tr>
      <th>날짜</th>
      <th>참여자 수</th>
    </tr>
  </thead>
  <tbody>
     <% @users_counts_daily.each do |v| %>
     <tr>
      <td>
       <span><%=v.created_date%></span>  
      </td> 
      <td>
        <span><%=v.total_count%></span>  
      </td>
    </tr>
    <% end %>
    <% @users_counts_sum.each do |v| %>
    <tr>
      <td>
       <span>총합계</span>  
      </td>
      <td>
        <span><%=v.total_count%></span>  
      </td>
    </tr>  
    <% end %>
  </tbody>
</table>